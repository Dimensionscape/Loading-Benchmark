<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="Loading Benchmark" package="LoadingBenchmark" version="1.0.0" company="Christopher Speciale" />
	
	<!-- output -->
	<app main="Main" file="LoadingBenchmark" path="bin" />
	
	<window background="#000000" fps="5" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />
	
	<!-- assets -->
	<assets path="assets" rename="" />
	<template path="\custom_assets\"/>
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>